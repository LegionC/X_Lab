interface vlan 35
 ip add 10.1.35.254 24 
 dhcp select relay
 dhcp relay server-ip 10.1.200.33

interface vlan 41
 ip add 10.1.41.254 24 
 dhcp select relay
 dhcp relay server-ip 10.1.200.33

interface vlan 42
 ip add 10.1.42.254 24 
 dhcp select relay
 dhcp relay server-ip 10.1.200.33

interface vlan 43
 ip add 10.1.43.254 24 
 dhcp select relay
 dhcp relay server-ip 10.1.200.33

interface vlan 44
 ip add 10.1.44.254 24 
